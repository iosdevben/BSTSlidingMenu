#include "Shared.xcconfig"

// Override here for Debug Configurations

CODE_SIGN_IDENTITY = iPhone Developer
CODE_SIGN_IDENTITY[sdk=iphoneos*] = iPhone Developer

COPY_PHASE_STRIP = NO
STRIP_INSTALLED_PRODUCT = NO
VALIDATE_PRODUCT = NO
